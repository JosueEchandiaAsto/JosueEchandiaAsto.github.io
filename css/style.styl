@import url("materialize.min.css");

nav
	background none
	box-shadow none
	i.large-icon
		font-size 3rem;
		position absolute
		top 10px
		left 15px
.header
	&__nav
		position absolute
		z-index 5
	&__title
		img
			width 100%
			height 100vh 
	&__text
		position absolute
		top 33vh
		text-align center
		width 100%
		background rgba(black, 0.62);
		padding 0 20px 10px 20px
		color white
		font-family: 'Open Sans', sans-serif;
		h4
			font-family: 'Open Sans Condensed', sans-serif;
